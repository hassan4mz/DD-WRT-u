/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "raisecom,msg1500", "ralink,mt7621-soc";
	model = "Raisecom MSG1500";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};
	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds: leds {
		compatible = "gpio-leds";

		wifi {
			gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
			label = "green:wifi";
			linux,default-trigger = "phy0tpt";
		};
	};

};


&nand {
	status = "okay";

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "u-boot";
			reg = <0x0 0x100000>;
			read-only;
		};

		partition@100000 {
			label = "SC PART_MAP";
			reg = <0x100000 0x100000>;
			read-only;
		};

		partition@200000 {
			label = "linux";
			reg = <0x200000 0x2c00000>;
		};

		partition@600000 {
			label = "ubi";
			reg = <0x600000 0x2800000>;
		};

		partition@2e00000 {
			label = "ddwrt";
			reg = <0x2e00000 0x1800000>;
		};

		factory: partition@4600000 {
			label = "factory";
			reg = <0x4600000 0x200000>;
			read-only;
		};
		
		partition@4800000 {
			label = "nvram";
			reg = <0x4800000 0x200000>;
		};

		partition@4A00000 {
			label = "reserved1";
			reg = <0x4A00000 0x3600000>;
			read-only;
		};
	};
};

&pcie {
	status = "okay";
};

&pcie0 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x8000>;
		mediatek,disable_2ghz = <1>;
		ieee80211-freq-limit = <5000000 6000000>;
	};
};

&pcie1 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0 0 0 0 0>;
		mediatek,disable_5ghz = <1>;
		mediatek,mtd-eeprom = <&factory 0x0>;
		ieee80211-freq-limit = <2400000 2500000>;
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "uart3", "uart2", "jtag", "wdt";
			ralink,function = "gpio";
		};
	};
};

&xhci {
	status = "okay";
};
