/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "raisecom,msg1500", "ralink,mt7621-soc";
	model = "Raisecom MSG1500";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x10000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		
		wlan2g {
			label = "sw2g";
			gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0radio";
		};

	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&nand {
38	        status = "okay";
39	
40	        partitions {
41	                compatible = "fixed-partitions";
42	                #address-cells = <1>;
43	                #size-cells = <1>;
44	
45	                partition@0 {
46	                        label = "u-boot";
47	                        reg = <0x0 0x100000>;
48	                        read-only;
49	                };
50	
51	                partition@100000 {
52	                        label = "SC PID";
53	                        reg = <0x100000 0x100000>;
54	                        read-only;
55	                };
56	
57	                partition@200000 {
58	                        label = "linux";
59	                        reg = <0x200000 0x2000000>;
60	                };
61	
62	                partition@600000 {
63	                        label = "ubi";
64	                        reg = <0x600000 0x1c00000>;
65	                };
66	
67	                partition@2f00000 {
68	                        label = "nvram";
69	                        reg = <0x2f00000 0x200000>;
70	                };
71	
72	                factory: partition@2e00000 {
73	                        label = "factory";
74	                        reg = <0x2e00000 0x100000>;
75	                        read-only;
76	                };
77	
78	                partition@3100000 {
79	                        label = "ddwrt";
80	                        reg = <0x3100000 0x4f00000>;
81	                };
82	        };
83	};

&pcie {
	status = "okay";
};

&pcie0 {
	mt76@0,0 {
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0000>;
		/*led {
			led-sources = <3>;
			led-active-low;
		};*/
	};
};


&raeth {
	status = "okay";
	mac-address = [00 00 00 00 00 00];
	gmac1-support = "rgmii-1";
	rgmii-mode-1 = "force-1000";
	gmac2-support = "rgmii-2";
	rgmii-mode-2 = "force-1000";
	gmac2-phy-address = <0>;
	gmac1_txq_num = <1>;
	gmac1_txq_txd_num = <1024>;
	gmac2_txq_num = <1>;
	gmac2_txq_txd_num = <1024>;
	num_rx_desc = <1024>;
};


&xhci {
	status = "okay";
};

&state_default {
	gpio {
		groups = "i2c", "jtag", "uart2", "uart3", "wdt";
		function = "gpio";
	};
};
