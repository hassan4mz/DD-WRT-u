/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "raisecom,msg1500", "ralink,mt7621-soc";
	model = "Raisecom MSG1500";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x10000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		wlan5g {
			label = "sw5g";
			gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1radio";
		};

		wlan2g {
			label = "sw2g";
			gpios = <&gpio0 4 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0radio";
		};

		usb {
			label = "usb";
			gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "usbport";
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};


&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw";
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x0 0x80000>; /* 512k */
			};
						
			partition@80000 {
				label = "Config";
				reg = <0x80000 0x80000>; /* 512k */
			};

			partition@100000 {
				label = "Factory";
				reg = <0x100000 0x40000>; /* 256k */
			};

			partition@140000 {
				label = "firmware";
				reg = <0x140000 0x1840000>; /* 24M */
			};		
			
			partition@1980000 {
				label = "Storage";
				reg = <0x1980000 0x400000>; /* 4096K */
			};

			partition@1d80000 {
				label = "RWFS";
				reg = <0x1d80000 0x6200000>;
			};

			partition@all {
				label = "ALL";
				reg = <0x0 0x7f80000>;
			};
		};
	};
};

&raeth {
	status = "okay";
	mac-address = [00 00 00 00 00 00];
	gmac1-support = "rgmii-1";
	rgmii-mode-1 = "force-1000";
	gmac2-support = "rgmii-2";
	rgmii-mode-2 = "force-1000";
	gmac2-phy-address = <0>;
	gmac1_txq_num = <1>;
	gmac1_txq_txd_num = <1024>;
	gmac2_txq_num = <1>;
	gmac2_txq_txd_num = <1024>;
	num_rx_desc = <1024>;
};

&pcie {
	status = "okay";
};


&xhci {
	status = "okay";
};

&state_default {
	gpio {
		groups = "i2c", "jtag", "uart2", "uart3", "wdt";
		function = "gpio";
	};
};
